{
    "extends": ["airbnb", "eslint:recommended", "plugin:react/recommended"],
    "installedESLint": true,
    "plugins": [
        "react",
        "html"
    ],
    "settings": {
      "react": {
        "version": "15.0"
      }
    },
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "parser": "babel-eslint",
    "rules": {
      "react/no-multi-comp": 0
    }
}
